{
    "name": "eso/doctrine-repository",
    "type": "library",
    "description": "An alternative doctrine entity base repository ",
    "keywords": ["php", "doctrine", "repository", "entity"],
    "homepage": "http://github.com/entering/doctrine-repository",
    "license": "MIT",
    "authors": [
        {
            "name": "Eduardo Oliveira",
            "email": "entering@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "doctrine/orm": ">=2.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=3.7",
        "squizlabs/php_codesniffer": "~1.5.0",
        "phpmd/phpmd": "~1.5.0",
        "eso/ireflection": "~0.1"
    },
    "autoload": {
        "psr-0": { "ESO\\Doctrine\\ORM": "src/" }
    }
}
